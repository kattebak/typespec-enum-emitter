{
	"name": "@kattebak/typespec-enum-emitter",
	"version": "1.0.0",
	"description": "TypeSpec emitter for JavaScript const enums",
	"type": "module",
	"main": "dist/index.js",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist",
		"tsp"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js",
			"typespec": "./tsp/main.tsp"
		}
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc --watch",
		"test": "npm run build && npm run lint && npm run build",
		"test:emit": "sp compile test/main.tsp --config test/tspconfig.yaml",
		"fix": "npx @biomejs/biome check --write",
		"lint": "npx @biomejs/biome check"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.7",
		"@typespec/compiler": "^1.4.0",
		"typescript": "^5.9.3"
	}
}
